[OPTIONS]

enable: 1
ebtables: 0

[RULES]

IN ACCEPT -dest 255.255.255.255 -p udp -dport 67 -log nolog # DHCP
IN ACCEPT -p udp -dport 53 -log nolog # DNS
IN ACCEPT -source 100.64.0.0/10 -p tcp -dport 8006 # Proxmox Web GUI
IN ACCEPT -source 100.64.0.0/10 -p tcp -dport 2222 # SSH

IN DROP -p icmp -log nolog # ICMP

IN DROP -log nolog
